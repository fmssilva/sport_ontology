PREFIX sports: <http://www.semanticweb.org/sports/ontology#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

# Test Query 1: Check for jersey number constraint violations
# Should return empty if all jersey numbers are in range [1-99]
SELECT ?player ?jersey WHERE {
  ?player a sports:Player ;
          sports:hasJerseyNumber ?jersey .
  FILTER (?jersey < 1 || ?jersey > 99)
}
LIMIT 10